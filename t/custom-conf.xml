<?xml version="1.0"?>
<!DOCTYPE fontconfig SYSTEM "fonts.dtd">
<!-- custom fontconfig configuration file -->
<fontconfig>
	<its:rules xmlns:its="http://www.w3.org/2005/11/its" version="1.0">
		<its:translateRule translate="no" selector="/fontconfig/*[not(self::description)]"/>
	</its:rules>

	<description>Example custom fontconfig configuration file</description>
	<dir prefix="cwd">t/fonts</dir>
	<cachedir prefix="cwd">tmp/fontconfig</cachedir>

</fontconfig>
